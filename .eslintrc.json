{
  "extends": [
    "plugin:react/recommended",
    "next/core-web-vitals",
    "eslint:recommended"
  ],
  "rules": {
    "jsx-a11y/anchor-is-valid": 0,
    "no-unused-vars": [
      "warn",
      {
        "ignoreRestSiblings": true
      }
    ],
    "react/react-in-jsx-scope": "off",
    "react/prop-types": ["off"]
  }
}
